{
  "name": "react-quiz-component",
  "version": "0.4.0",
  "description": "React Quiz Component",
  "main": "lib/Quiz.js",
  "scripts": {
    "start": "concurrently \"npm run lib:watch\" \"npm run docs\"",

    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development --hot --open --port 4697",
    "docs:prod": "webpack --mode production",
    "lint": " ./node_modules/.bin/eslint src --ext .jsx --ext .js --fix",

    "build": "npm i -dev && webpack --mode production",
    "heroku-postbuild": "npm run build"

  },
  
  "engines":{
    "node":"v16.7.0"
  },

  "keywords": [
    "react",
    "reactjs",
    "react-quiz-component",
    "react-quiz",
    "quiz"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  
  "author": "WONG, Wing Kam <wingkwong.code@gmail.com>",
  "homepage": "https://github.com/wwjohndavid/TriviaProject",
  "repository": {
    "type": "git",
    "url": "https://github.com/wwjohndavid/TriviaProject.git"
  },
  "dependencies": {
    "@webpack-cli/serve": "^1.6.1",
    "dompurify": "^2.0.8",
    "marked": "4.0.10",
    "react-quiz-component": "^0.4.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^3.5.1",
    "css-loader": "^3.4.2",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "5.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.0",
    "style-loader": "^0.21.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
